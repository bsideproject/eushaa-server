{
	"singleQuote": true,
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"trailingComma": "es5",
	"arrowParens": "always",
	"endOfLine": "lf",
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve"
}
